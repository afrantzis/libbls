prefix=@PREFIX@
exec_prefix=@PREFIX@
libdir=@PREFIX@/lib
includedir=@PREFIX@/include

lfs_cflags=@LFS_CFLAGS@
lfs_linkflags=@LFS_LINKFLAGS@

Name: libbls
Description: Efficient editable data buffer
Version: @LIBBLS_VERSION@
Libs: -L${libdir} -lbls-@LIBBLS_VERSION_NO_PATCH@ ${lfs_linkflags}
Cflags: -I${includedir}/bls-@LIBBLS_VERSION_NO_PATCH@ ${lfs_cflags}
